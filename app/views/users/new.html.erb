<div class="container">
  <div class="jumbotron">
    <div class="form-group">
      <%= form_for @user do |f| %>
          <%= render :partial => 'form', :locals => {:f => f} %>
        <%= f.submit 'Sign Up', :class => "btn btn-primary mt-2" %>
      <% end %>
    </div>
          <%= link_to 'Sign up with Facebook', '/auth/facebook', :class => 'btn btn-primary mt2' %>
  </div>
</div>
